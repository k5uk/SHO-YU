<!-- Contents Field Start -->
<div class="Contents-Field mdl-color--brown-50">
    <!-- Main Contents Field Start -->
    <div class="Main-Contents-Field">
        
        <div class="links-section-title mdl-typography--display-1-color-contrast">オンライン将棋サイト</div>
        
        <!-- Multiple Card Container Start -->
        <div class="Multiple-Card-Container mdl-grid">

            <div class="mdl-card mdl-shadow--4dp mdl-cell mdl-cell--3-col mdl-color--lime-50">
                <div class="mdl-card__media">
                    <%= image_tag "link/wars.png" %>
                </div>
            	  <div class="mdl-card__title">
            	      <h4>将棋ウォーズ</h4>
            	  </div>
            	  <div class="mdl-card__supporting-text">
            	      <span class="mdl-typography--font-light mdl-typography--subhead">
            	          初級者から高段者まで幅広いユーザーが存在<br />
            	          戦法や囲いが完成するとド派手な演出が出る<br />
            	          今最もホットな将棋アプリ？
            	      </span>
            	  </div>
            	  <div class="mdl-card__actions">
            	       <%= link_to "http://shogiwars.heroz.jp/", 
            	       { :class => "links-link mdl-button mdl-js-button mdl-typography--text-uppercase" }  do %>
            	          <span class="icon-link">サイトへアクセス
            	              <i class="material-icons">chevron_right</i>
            	          </span>
            	       <% end %>
            	  </div>
            </div>
    
        	<div class="mdl-card mdl-shadow--4dp mdl-cell mdl-cell--3-col mdl-color--lime-50">
