        	            <span class="icon-link">サイトへアクセス
        	                <i class="material-icons">chevron_right</i>
        	            </span>
        	        <% end %>
        	    </div>
        	</div>
  
        </div>
        <!--Multiple Card Container END -->
    
    </div>
    <!-- Main Contents Field END -->

</div>
<!-- Contents Field END -->