<!-- Contents Field Start -->
<div class="Contents-Field mdl-color--brown-50">
    <!-- Main Contents Field Start -->
    <div class="Main-Contents-Field">
        <!-- Multiple Card Container Start -->
        <div class="Multiple-Card-Container mdl-grid">
            <div class="cards-title-area">
                <div class="cards-section-title mdl-typography--display-1-color-contrast">オンライン将棋サイト</div>
            </div>
            <div class="mdl-card mdl-shadow--4dp mdl-cell mdl-cell--3-col mdl-color--lime-50">
                <div class="mdl-card__media">
                    <%= image_tag "link/wars.png" %>
                </div>
            	  <div class="mdl-card__title">
            	      <h4 class="mdl-card__title-text">将棋ウォーズ</h4>
            	  </div>
        	      <div class="mdl-card__supporting-text"></div>            	  
            	  <div class="mdl-card__actions">
            	       <%= link_to "http://shogiwars.heroz.jp/", 
            	       { :class => "multiple-link mdl-button mdl-js-button mdl-typography--text-uppercase" }  do %>
            	          <span class="icon-link">サイトへアクセス
            	              <i class="material-icons">chevron_right</i>
            	          </span>
            	       <% end %>
            	  </div>
            </div>
    
        	<div class="mdl-card mdl-shadow--4dp mdl-cell mdl-cell--3-col mdl-color--lime-50">
        	    <div class="mdl-card__media">
        	        <%= image_tag "link/24.png" %>
        	    </div>
        	    <div class="mdl-card__title">
        	        <h4 class="mdl-card__title-text">将棋倶楽部24</h4>
        	    </div>
        	   <div class="mdl-card__supporting-text"></div>        	    
        	    <div class="mdl-card__actions">
        	        <%= link_to "http://www.shogidojo.com/", 
        	        { :class => "multiple-link mdl-button mdl-js-button mdl-typography--text-uppercase" }  do %>
        	            <span class="icon-link">サイトへアクセス
        	                <i class="material-icons">chevron_right</i>
        	            </span>
        	        <% end %>
        	    </div>
        	</div>
    
    		<div class="mdl-card mdl-shadow--4dp mdl-cell mdl-cell--3-col mdl-color--lime-50">
    		    <div class="mdl-card__media">
    		        <%= image_tag "link/hangemae.png" %>
    		    </div>
    		    <div class="mdl-card__title">
    		        <h4 class="mdl-card__title-text">ハンゲーム将棋弐</h4>
    		    </div>
        	   <div class="mdl-card__supporting-text"></div>    		    
    		    <div class="mdl-card__actions">
    		       <%= link_to "http://casual.hangame.co.jp/shougi2/", 
    		       { :class => "multiple-link mdl-button mdl-js-button mdl-typography--text-uppercase" }  do %>
    		          <span class="icon-link">サイトへアクセス
    		              <i class="material-icons">chevron_right</i>
    		          </span>
    			      <% end %>
    		    </div>
    		</div>
    
        	<div class="mdl-card mdl-shadow--4dp mdl-cell mdl-cell--3-col mdl-color--lime-50">
        	    <div class="mdl-card__media">
        	        <%= image_tag "link/81.png" %>
        	    </div>
        	    <div class="mdl-card__title">
        	        <h4 class="mdl-card__title-text">８１道場</h4>
        	    </div>
        	   <div class="mdl-card__supporting-text"></div>
